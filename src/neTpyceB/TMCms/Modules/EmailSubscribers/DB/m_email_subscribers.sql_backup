CREATE TABLE `1travel`.`m_email_subscribers` ( `id` INT UNSIGNED NOT NULL AUTO_INCREMENT , `ip` VARCHAR(45) NOT NULL , `ts` INT UNSIGNED NOT NULL , `user_id` INT UNSIGNED NOT NULL , `email` VARCHAR(255) NOT NULL , `receive_news` TINYINT(1) UNSIGNED NOT NULL DEFAULT '0' , `receive_ads` TINYINT(1) UNSIGNED NOT NULL DEFAULT '0' , `notes` VARCHAR(255) NOT NULL , PRIMARY KEY (`id`)) ENGINE = InnoDB;